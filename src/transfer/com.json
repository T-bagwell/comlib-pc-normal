    {
      "id": "dataSource",
      "title": "设置数据源",
      "schema": {
        "type": "object",
        "properties": {
          "dataSource": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "disabled": {
                  "type": "boolean"
                }
              }
            }
          },
          "target": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "getData",
      "title": "获取数据",
      "schema": {
        "type": "any"
