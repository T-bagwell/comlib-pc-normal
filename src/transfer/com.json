            }
          }
        }
      }
    },
    {
      "id": "onChange",
      "title": "目标数据改变",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          }
