{
  "title": "标签页",
  "namespace": "mybricks.normal-pc.tabs",
  "version": "1.0.0",
  "description": "选项卡切换组件",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "previous",
      "title": "上一页",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "next",
      "title": "下一页",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "active",
      "title": "激活tab项",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "outActiveTab",
      "title": "获取激活tab项",
      "rels": [
        "outActiveTab"
      ],
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "tabClick",
      "title": "标签页点击",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "outActiveTab",
      "title": "激活tab项输出",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "title": "标签页id",
            "type": "string"
          },
          "name": {
            "title": "标签页标题",
            "type": "string"
          },
          "permissionKey": {
            "title": "标签页权限key",
            "type": "string"
          }
        }
      }
    }
  ],
  "slots": [
    {
      "id": "tab0",
      "title": "标签页【1】",
      "type": "scope",
      "inputs": [
        {
          "id": "tab0_into",
          "title": "显示",
          "schema": {
            "type": "any"
          }
        },
        {
          "id": "tab0_leave",
          "title": "隐藏",
          "schema": {
            "type": "any"
          }
        }
      ]
    }
  ]
}