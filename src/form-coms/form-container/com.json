{
  "title": "表单容器",
  "namespace": "mybricks.normal-pc.form-container",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.0",
  "description": "表单容器",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "inputs": [
    {
      "id": "initial",
      "title": "初始化表单",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "setFieldsValue",
      "title": "设置表单值",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "submit",
      "title": "提交表单",
      "rels": [
        "onFinish"
      ],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "resetFields",
      "title": "重置表单",
      "rels": [
        "onResetFinish"
      ],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onInitial",
      "title": "初始化表单",
      "schema": {
        "type": "object",
        "properties": {
          "values": {
            "title": "表单值",
            "type": "object",
            "properties": {}
          }
        }
      }
    },
    {
      "id": "onResetFinish",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    }
  ],
  "slots": [
    {
      "id": "content",
      "title": "拖入表单项",
      "schema": "mybricks.normal-pc.form-container/form-item",
      "type": "scope"
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}
