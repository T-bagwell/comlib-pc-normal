{
  "title": "表单容器",
  "namespace": "mybricks.normal-pc.form-container",
  "author": "ZhuPengqiang",
  "author_name": "朱鹏强",
  "version": "1.0.0",
  "description": "表单容器",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors/index.ts",
  "inputs": [
    {
      "id": "setFieldsValue",
      "title": "设置表单值",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "submit",
      "title": "提交表单",
      "rels": [
        "onFinish"
      ],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "resetFields",
      "title": "重置表单",
      "rels": [
        "onResetFinish"
      ],
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onResetFinish",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onClickSubmit",
      "title": "点击提交",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "onClickCancel",
      "title": "点击取消",
      "schema": {
        "type": "any"
      }
    }
  ],
  "_inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    }
  ],
  "slots": [
    {
      "id": "content",
      "title": "表单容器",
      "schema": "mybricks.normal-pc.form-container/form-item",
      "type": "scope",
      "inputs": [
        {
          "id": "setFieldsValue",
          "title": "表单值输入",
          "schema": {
            "type": "object",
            "properties": {}
          }
        }
      ]
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}
