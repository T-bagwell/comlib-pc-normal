{
  "title": "文本框",
  "namespace": "mybricks.normal-pc.form-text",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.0",
  "description": "文本框",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setInvisible",
      "title": "设置隐藏",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setVisible",
      "title": "设置显示",
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onChange",
      "title": "值发生改变",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "schema": {
        "type": "string"
      }
    }
  ],
  "_valueSchema": {
    "type": "string"
  },
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}