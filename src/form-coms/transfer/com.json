{
  "title": "穿梭框",
  "namespace": "mybricks.normal-pc.transfer",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.0",
  "description": "组件描述",
  "docs": "组件使用说明文档链接",
  "icon": "./icon.svg",
  "runtime": "./runtime.tsx",
  "data": "./data.json",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "setSource",
      "title": "设置数据源",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          }
        }
      },
      "desc": "给穿梭框设置数据源"
    },
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "desc": "给穿梭框设置目标值"
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "dataOut"
      ],
      "desc": "获取穿梭框目标值"
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      },
      "desc": "重置穿梭框目标值"
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "desc": "禁用穿梭框组件"
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "desc": "启用穿梭框组件"
    }
  ],
  "outputs": [
    {
      "id": "dataOut",
      "title": "数据输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "onChange",
      "title": "目标数据改变",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          }
        }
      }
    }
  ],
  "slots": [],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}