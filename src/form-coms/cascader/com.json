{
  "title": "级联选择",
  "namespace": "mybricks.normal-pc.cascader",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.0.0",
  "description": "级联选择",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "schema": {
        "type": "array",
        "items": {
          "type": "any"
        }
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setVisible",
      "title": "设置显示/隐藏",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setOptions",
      "title": "输入数据源",
      "schema": {
        "title": "输入数据源数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "value": {
              "title": "值",
              "type": "any"
            },
            "label": {
              "title": "名称",
              "type": "string"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {}
              }
            }
          }
        }
      }
  }
  ],
  "outputs": [
    {
      "id": "onChange",
      "title": "值变化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}
